<resources>
    <!-- Base application theme. -->
    <style name="Theme.OwOUploader" parent="Theme.AppCompat.DayNight.NoActionBar">
        <item name="backgroundColorPrimary">@color/dark_gray</item>
        <item name="backgroundColorSecondary">@color/darker_gray</item>

        <item name="textColorPrimary">@color/light_gray</item>
        <item name="textColorSecondary">@color/gray</item>

        <item name="brandColor">@color/owo_blue</item>

        <item name="tabColorSelected">?attr/textColorPrimary</item>
        <item name="tabColor">?attr/textColorSecondary</item>

        <item name="fabColor">@color/white</item>
        <item name="fabColorSecondary">@color/light_gray</item>
        <item name="fabColorIcon">@color/dark_gray</item>
    </style>
</resources>